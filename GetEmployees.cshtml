@model List<lab1_ef.Employee>

<div><img src="~/images/1.jpg" /></div>
@using (Html.BeginForm())
{
    <h2>Работники</h2>
    <div>
        <ul>
            <li><p>ID, Имя</p></li>
            @foreach (var type in Model)
            {
                <li>
                    <p>@type.EmployeeId, @type.Name</p>
                </li>
            }
        </ul>
    </div>
}