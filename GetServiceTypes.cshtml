@model List<lab1_ef.ServiceType>

<div><img src="~/images/1.jpg" /></div>
@using (Html.BeginForm())
{
    <h2>Сервис в наличии</h2>
    <div>
        <ul>
            <li><p>ID, Имя, Прайс</p></li>
            @foreach (var type in Model)
            {
                <li>
                    <p>@type.ServiceTypeId, @type.Name, @type.Cost</p>
                </li>
            }
        </ul>
    </div>
}
